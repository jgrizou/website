 author = {Grizou, Jonathan and Lopes, Manuel and Oudeyer, Pierre-Yves},
 booktitle = {HRI 2014 Pioneers Workshop},
 title = {{R}obot {L}earning from {U}nlabelled {T}eaching {S}ignals},
 year = {2014}
}

